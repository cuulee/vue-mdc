{
  "name": "ssr-demo",
  "version": "1.0.0",
  "description": "A vue-mdc SSR Demo",
  "private": true,
  "main": "./src/index.js",
  "config": {
    "env": "development",
    "serverHost": "127.0.0.1",
    "serverPort": 3000,
    "publicPath": "/"
  },
  "scripts": {
    "build": "npm run build:client; npm run build:server",
    "build:client": "node --harmony ./scripts/build.js client",
    "build:server": "node --harmony ./scripts/build.js server",
    "start": "node --harmony ./scripts/start"
  },
  "author": "Kristijan Sedlak (Xpepermint)",
  "license": "MIT",
  "dependencies": {
    "express": "5.0.0-alpha.2",
    "express-vue-builder": "^2.15.0",
    "vue": "^2.1.3",
    "vue-builder": "^1.7.0",
    "vue-server-renderer": "^2.1.3",
    "@material/checkbox": "^0.1.0",
    "@material/drawer": "^0.1.0",
    "@material/form-field": "^0.1.0",
    "@material/icon-toggle": "^0.1.0",
    "@material/list": "^0.1.0",
    "@material/ripple": "^0.1.0",
    "@material/snackbar": "^0.1.0",
    "@material/typography": "^0.1.0",
    "@v-material/checkbox": "^0.1.0",
    "@v-material/drawer": "^0.1.0",
    "@v-material/form-field": "^0.1.0",
    "@v-material/icon-toggle": "^0.1.0",
    "@v-material/ripple": "^0.1.0",
    "@v-material/snackbar": "^0.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "css-loader": "^0.26.0",
    "express-vue-dev": "^1.12.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "inline-environment-variables-webpack-plugin": "^1.1.0",
    "node-sass": "^4.1.1",
    "sass-loader": "^4.1.0",
    "vue-loader": "^10.0.1",
    "vue-template-compiler": "^2.1.3",
    "webpack": "2.1.0-beta.27"
  }
}
